    agent any
    stages {
        stage('Checkout') {
            steps {
                git url: 'https://github.com/ankitamohanty1509/python.git', branch: 'main'
            }
        }
        stage('Install Dependency') {
            steps {
                echo "installed dependency"
            }
        }
        
        stage('Run Tests') {
            steps {
                echo "test ran" 
                
                    
                
            }
        }
    }
}
